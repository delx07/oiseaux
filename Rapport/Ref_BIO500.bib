@article{lomer_b_2001,
	title = {B {\textless}span style="font-variant:small-caps;"{\textgreater}{IOLOGICAL}{\textless}/span{\textgreater} {C} {\textless}span style="font-variant:small-caps;"{\textgreater}{ONTROL} {OF}{\textless}/span{\textgreater} {L} {\textless}span style="font-variant:small-caps;"{\textgreater}{OCUSTS} {AND}{\textless}/span{\textgreater} {G} {\textless}span style="font-variant:small-caps;"{\textgreater}{RASSHOPPERS}{\textless}/span{\textgreater}},
