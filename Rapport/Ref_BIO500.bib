@article{rabosky_minimal_2015,
	title = {Minimal effects of latitude on present-day speciation rates in {New} {World} birds},
	volume = {282},
	issn = {0962-8452, 1471-2954},
	url = {https://royalsocietypublishing.org/doi/10.1098/rspb.2014.2889},
	doi = {10.1098/rspb.2014.2889},
	abstract = {The tropics contain far greater numbers of species than temperate regions, suggesting that rates of species formation might differ systematically between tropical and non-tropical areas. We tested this hypothesis by reconstructing the history of speciation in New World (NW) land birds using BAMM, a Bayesian framework for modelling complex evolutionary dynamics on phylogenetic trees. We estimated marginal distributions of present-day speciation rates for each of 2571 species of birds. The present-day rate of speciation varies approximately 30-fold across NW birds, but there is no difference in the rate distributions for tropical and temperate taxa. Using macroevolutionary cohort analysis, we demonstrate that clades with high tropical membership do not produce species more rapidly than temperate clades. For nearly any value of present-day speciation rate, there are far more species in the tropics than the temperate zone. Any effects of latitude on speciation rate are marginal in comparison to the dramatic variation in rates among clades.},
	language = {en},
	number = {1809},
	urldate = {2024-04-24},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	author = {Rabosky, Daniel L. and Title, Pascal O. and Huang, Huateng},
	month = jun,
	year = {2015},
	pages = {20142889},
	file = {Available Version (via Google Scholar):C\:\\Users\\felix\\Zotero\\storage\\US68CFPY\\rspb.2014.html:text/html;Texte intégral:C\:\\Users\\felix\\Zotero\\storage\\B5TMC9GL\\Rabosky et al. - 2015 - Minimal effects of latitude on present-day speciat.pdf:application/pdf},
}

